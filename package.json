{
  "name": "salesloft-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "cz",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\""
  },
  "dependencies": {
    "autoprefixer": "^10.2.1",
    "next": "10.0.5",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tx,css,md}": "prettier --write"
  }
}
